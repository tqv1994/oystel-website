{
  "name": "oysteo-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "svelte-kit dev",
    "dev:dev": "env $(cat .env.dev | grep -v \"#\" | xargs) svelte-kit dev --host",
    "dev:staging": "env $(cat .env.staging | grep -v \"#\" | xargs) svelte-kit dev --host",
    "dev:prod": "env $(cat .env.prod | grep -v \"#\" | xargs) svelte-kit dev --host",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "fix": "node _smui/vite-workaround.js",
    "theme": "smui-theme compile static/smui.css -i src/theme",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@beyonk/svelte-carousel": "https://github.com/su93rheroes/svelte-carousel.git",
    "@firebase/app-types": "^0.7.0",
    "@rollup/pluginutils": "^4.2.0",
    "@smui-extra/autocomplete": "^6.0.0-beta.15",
    "@smui/button": "^6.0.0-beta.15",
    "@smui/card": "^6.0.0-beta.15",
    "@smui/checkbox": "^6.0.0-beta.15",
    "@smui/data-table": "^6.0.0-beta.15",
    "@smui/dialog": "^6.0.0-beta.15",
    "@smui/drawer": "^6.0.0-beta.15",
    "@smui/form-field": "^6.0.0-beta.15",
    "@smui/icon-button": "^6.0.0-beta.15",
    "@smui/layout-grid": "^6.0.0-beta.15",
    "@smui/linear-progress": "^6.0.0-beta.15",
    "@smui/list": "^6.0.0-beta.15",
    "@smui/menu": "^6.0.0-beta.15",
    "@smui/radio": "^6.0.0-beta.15",
    "@smui/ripple": "^6.0.0-beta.15",
    "@smui/select": "^6.0.0-beta.15",
    "@smui/tab": "^6.0.0-beta.15",
    "@smui/tab-bar": "^6.0.0-beta.15",
    "@smui/textfield": "^6.0.0-beta.15",
    "@smui/tooltip": "^6.0.0-beta.15",
    "@smui/top-app-bar": "^6.0.0-beta.15",
    "@sveltejs/adapter-vercel": "^1.0.0-next.54",
    "@sveltejs/kit": "^1.0.0-next.341",
    "@types/cookie": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "blurhash": "^1.1.5",
    "cookie": "^0.5.0",
    "dayjs": "^1.11.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "firebase": "^9.8.0",
    "lodash.debounce": "^4.0.8",
    "meilisearch": "^0.25.0",
    "prettier": "~2.6.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.49.9",
    "slugify": "^1.6.5",
    "smui-theme": "^6.0.0-beta.15",
    "svelte": "^3.46.4",
    "svelte-calendar": "^3.1.6",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "yup": "^0.32.11"
  },
  "type": "module",
  "dependencies": {
    "svelte-markdown": "^0.2.2",
    "svelte-select": "^4.4.7"
  }
